<% title "SignUp"%>
<%= form_for @user do |f| %>
				<p>	<%= f.label :name,"Name" %></br>
				<%= f.text_field :name %></p>
				<p>
				<%= f.label :roll,"EnrollmentId"%></br>
				<%= f.text_field :roll%></p>
				<p>
				<%= f.label :email,"EmailId"%></br>
				<%= f.text_field :email%></p>
				<p>
				<%= f.label :batch,"Batch" %></br>
				<%= f.text_field :batch %></p>
				<%= f.submit "Sign Up"%>
<% end %>

